# Oh My Zsh
export ZSH="$HOME/.oh-my-zsh"
# ZSH_THEME="dst"
plugins=(git zsh-autosuggestions F-Sy-H zsh-fzf-history-search)
source "$ZSH/oh-my-zsh.sh"

# Modular Config
for file in "$HOME/.config/zsh/"*.zsh; do
  source "$file"
done

# Starship
eval "$(starship init zsh)"

# Zoxide
eval "$(zoxide init zsh)"

# Status
uptime
